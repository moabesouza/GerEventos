@page
@using GerEventos.Localization
@using GerEventos.Pages.Produtor
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<GerEventosResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Produtor" asp-page="/Produtor/CreateModal">
    @Html.AntiForgeryToken()
    <abp-modal>
        <abp-modal-header title="@L["Novo produtor"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel | AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
